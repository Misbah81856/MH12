fetch('https://crudcrud.com/api/***/todos', {
  headers: { "Content-Type": "application/json; charset=utf-8" },
  method: 'POST',
  body: JSON.stringify({
    name: 'Write Tests',
    done: false
  })
})
.then(response => response.json())
.then(data => console.log(data))
{
  "_id":"5d20d8cfe6280703e8ec06d9",
  "name":"Write Tests",
  "done":false
}
